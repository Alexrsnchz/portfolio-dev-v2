{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],

  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@icons/*": ["src/assets/icons/*"],
      "@stack/*": ["src/assets/stack/*"],
    },
    "typeRoots": ["src/types", "node_modules/@types"]
  }
}

